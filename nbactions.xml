<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath be.belgif.vocgen.Main -f ldp.ttl -n http://www.w3.org/ns/ldp# -p ldp  -d https://www.w3.org/TR/ldp/  -s "LDP" -l "Linked Data Platform" -a "Bart Hanssens"</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>/home/bart/Desktop</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath be.belgif.vocgen.Main -f ldp.ttl -n http://www.w3.org/ns/ldp# -p ldp  -d https://www.w3.org/TR/ldp/  -s "LDP" -l "Linked Data Platform" -a "Bart Hanssens"</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>/home/bart/Desktop</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath be.belgif.vocgen.Main -f ldp.ttl -n http://www.w3.org/ns/ldp# -p ldp  -d https://www.w3.org/TR/ldp/  -s "LDP" -l "Linked Data Platform" -a "Bart Hanssens"</exec.args>
                <exec.executable>java</exec.executable>
                <exec.workingdir>/home/bart/Desktop</exec.workingdir>
            </properties>
        </action>
    </actions>
